{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["dist"],
    "compilerOptions": {
        "baseUrl": ".",
        /*"paths": {
            "@*": [src/*],
        },*/
        "paths": {
            "@components/*": ["./src/components/*"],
            "@images/*": ["./src/assets/images/*"],
            "@interfaces/*": ["./src/interfaces/*"],
            "@layouts/*": ["./src/layouts/*"],
            "@utils/*": ["./src/utils/*"]
        }
    }
}
